{
    "name": "tomphp/context-logger",
    "description": "A PSR-3 compliant logger decorator which allows context metadata to be built up.  ",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/tomphp/php-context-logger",
    "keywords": ["log", "logger", "psr7", "psr-7"],
    "authors": [
        {
            "name": "Tom Oram",
            "email": "tom@x2k.co.uk",
            "homepage": "https://github.com/tomphp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "friendsofphp/php-cs-fixer": "2.0.0-RC"
    },
    "autoload": {
        "psr-4": {
            "TomPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"]
    }
}
